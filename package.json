{
  "scripts": {
    "dev:api": "nodemon --ext \"ts\" --exec \"ts-node app/worker.ts\" --watch \"**/*.ts\"",
    "dev:worker": "nodemon --ext \"ts\" --exec \"ts-node app/worker.ts\" --watch \"**/*.ts\"",
    "dev:importer": "nodemon --ext \"ts\" --exec \"ts-node app/importer.ts\" --watch \"**/*.ts\"",
    "test": "jest"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.477.0",
    "dotenv": "^16.3.1",
    "ethers": "^6.9.0",
    "express": "^4.18.2",
    "prisma": "^5.7.1"
  },
  "devDependencies": {
    "@automock/jest": "^2.1.0",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.11",
    "@types/node": "^20.10.5",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/"
    ]
  }
}
